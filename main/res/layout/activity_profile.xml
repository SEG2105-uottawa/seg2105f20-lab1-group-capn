<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <LinearLayout
        android:layout_width="402dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Space
            android:layout_width="match_parent"
            android:layout_height="141dp"
            android:visibility="invisible" />

        <androidx.gridlayout.widget.GridLayout
            android:layout_width="match_parent"
            android:layout_height="400dp"
            app:columnCount="3"
            app:rowCount="2">

            <ImageView
                android:id="@+id/teamid00"
                android:layout_width="134dp"
                android:layout_height="200dp"
                app:layout_column="0"
                app:layout_row="0"
                app:layout_columnSpan="1"
                app:layout_rowSpan="1"
                app:srcCompat="@drawable/ic_logo_00" />

            <ImageView
                android:id="@+id/teamid03"
                android:layout_width="134dp"
                android:layout_height="200dp"
                app:layout_column="0"
                app:layout_row="1"
                app:layout_columnSpan="1"
                app:layout_rowSpan="1"
                app:srcCompat="@drawable/ic_logo_03" />

            <ImageView
                android:id="@+id/teamid01"
                android:layout_width="134dp"
                android:layout_height="200dp"
                android:contentDescription="Logo2"
                app:layout_column="1"
                app:layout_row="0"
                app:layout_columnSpan="1"
                app:layout_rowSpan="1"
                app:srcCompat="@drawable/ic_logo_01" />

            <ImageView
                android:id="@+id/teamid04"
                android:layout_width="134dp"
                android:layout_height="200dp"
                app:layout_column="1"
                app:layout_row="1"
                app:layout_columnSpan="1"
                app:layout_rowSpan="1"
                app:srcCompat="@drawable/ic_logo_04" />

            <ImageView
                android:id="@+id/teamid02"
                android:layout_width="134dp"
                android:layout_height="200dp"
                app:layout_column="2"
                app:layout_row="0"
                app:layout_columnSpan="1"
                app:layout_rowSpan="1"
                app:srcCompat="@drawable/ic_logo_02" />

            <ImageView
                android:id="@+id/teamid05"
                android:layout_width="134dp"
                android:layout_height="200dp"
                app:layout_column="2"
                app:layout_row="1"
                app:layout_columnSpan="1"
                app:layout_rowSpan="1"
                app:srcCompat="@drawable/ic_logo_05" />
        </androidx.gridlayout.widget.GridLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="135dp" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>